# Environment Variables Template
# COPY THIS TO .env - DO NOT EDIT THIS FILE WITH SECRETS

# Server Configuration
PORT=3000
NODE_ENV=development

# Facebook Messenger
FACEBOOK_PAGE_ACCESS_TOKEN=your_page_access_token_here
FACEBOOK_VERIFY_TOKEN=your_verify_token_here

# Google Cloud Configuration
GOOGLE_CREDENTIALS_JSON='{ ...content of google-credentials.json... }'
GOOGLE_CREDENTIALS_PATH=./credentials/google-credentials.json
GOOGLE_CALENDAR_ID=primary
GOOGLE_SHEET_ID=your_spreadsheet_id_here

# Clinic Configuration
CLINIC_NAME=Smile Dental Clinic
CLINIC_TIMEZONE=Asia/Manila
CLINIC_BUSINESS_HOURS_START=09:00
CLINIC_BUSINESS_HOURS_END=18:00
CLINIC_SLOT_DURATION=30
CLINIC_DAYS_ADVANCE_BOOKING=30

# Optional: OpenAI for natural language processing
OPENAI_API_KEY=sk-...

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_service_key
